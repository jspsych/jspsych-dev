{
    "name": "{npmPackageName}",
    "version": "0.0.1",
    "description": "{description}",
    "unpkg": "dist/index.browser.min.js",
    "files": [
      "index.js",
      "dist"
    ],
    "scripts": {
      "build": "babel index.js --presets @babel/preset-env,minify --source-maps --out-file dist/index.browser.min.js",
      "build:watch": "npm run build -- --watch",
      "update-readme": "node readme-scripts/generate-docs.js && node readme-scripts/filter-interfaces-docs.js && node readme-scripts/filter-timeline-docs.js && node readme-scripts/filter-timelineUnits-docs.js && node readme-scripts/filter-utils-docs.js && node readme-scripts/update-readme.js"
    },
    "repository": {
      "type": "git",
      "url": "{gitRootUrl}",
      "directory": "{packageDir}"
    },
    "keywords": [
      "jsPsych"
    ],
    "author": {
      "name": "{author}",
      "url": "{authorUrl}"
    },
    "license": "MIT",
    "bugs": {
      "url": "{gitRootHttpsUrl}/issues"
    },
    "homepage": "{documentationUrl}",
    "devDependencies": {
      "@jspsych/config": "^3.2.2",
      "jspsych": "^8.0.0"
    }
  }